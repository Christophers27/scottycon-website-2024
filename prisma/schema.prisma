generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model orders {
  order_id          String @id
  shippingfirstname String
  shippinglastname  String
  shippingemail     String
  shippingphone     String
  productoptions    String
  productquantity   Int
  checkedin         Int    @default(0)
}
